{
  "name": "web",
  "engines": {
    "node": "10.5.0"
  },
  "scripts": {
    "start": "concurrently \"yarn client\" \"yarn server\"",
    "client": "BROWSER=none yarn --cwd client start",
    "server": "docker-compose up -d web",
    "postinstall": "cd client && npm install && npm run build && cp -a build/. ../public"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "dependencies": {
  }
}
